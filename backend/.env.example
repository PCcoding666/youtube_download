# ====================================
# YouTube Transcriber Configuration
# ====================================

# Qwen API Key (for AI transcription and summarization)
QWEN_API_KEY=your_qwen_api_key_here

# Aliyun OSS Configuration
ALIYUN_ACCESS_KEY_ID=your_aliyun_access_key_id_here
ALIYUN_ACCESS_KEY_SECRET=your_aliyun_access_key_secret_here
ALIYUN_OSS_ENDPOINT=oss-cn-hangzhou.aliyuncs.com
ALIYUN_OSS_BUCKET=your_oss_bucket_name

# Qwen API Configuration
QWEN_API_BASE=https://dashscope-intl.aliyuncs.com/compatible-mode/v1/chat/completions

# Security Settings
SECRET_KEY=your_secret_key_here

# Application Configuration
STORAGE_DIR=./storage
LOG_LEVEL=INFO
TEMP_DIR=/tmp/video_analysis

# Optional: Direct Proxy for yt-dlp (if not using AgentGo)
# YOUTUBE_PROXY=http://127.0.0.1:33210,socks5://127.0.0.1:33211

# ====================================
# AgentGo Configuration (Primary YouTube Access Solution)
# ====================================
# Get API key from https://docs.agentgo.live/
# AgentGo provides cloud browsers with built-in proxy for YouTube access

AGENTGO_API_KEY=your_agentgo_api_key_here
AGENTGO_API_URL=https://api.browsers.live
# Default region for browser session: us, uk, de, fr, jp, sg, in, au, ca
AGENTGO_REGION=us

# YouTube credentials for AgentGo login
# Used to authenticate and extract cookies for yt-dlp
YOUTUBE_EMAIL=your_youtube_email_here
YOUTUBE_PASSWORD=your_youtube_password_here

# ====================================
# Intelligent Geo-Routing Configuration
# ====================================
# Automatically route requests to matching AgentGo regions based on user IP

# Enable/disable intelligent geo-routing (default: true)
ENABLE_GEO_ROUTING=true

# Optional: Path to MaxMind GeoLite2-Country database for local IP lookup
# Download from: https://dev.maxmind.com/geoip/geolite2-free-geolocation-data
# If not provided, falls back to online IP-API service
# GEOIP_DB_PATH=/path/to/GeoLite2-Country.mmdb

# ====================================
# Task Configuration
# ====================================
MAX_VIDEO_DURATION=600
TRANSCRIPTION_TIMEOUT=300
POLL_INTERVAL=5

# CORS Configuration
CORS_ORIGINS=http://localhost:5173,http://localhost:3000
