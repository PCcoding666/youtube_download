
# ====================================
# YouTube Transcriber Configuration
# ====================================

# API密钥
QWEN_API_KEY=your_qwen_api_key_here
TRANSCRIPT_SERVICE_API_KEY=your_transcript_service_api_key_here
HF_TOKEN=your_huggingface_token_here

# 阿里云OSS配置
ALIYUN_ACCESS_KEY_ID=your_aliyun_access_key_id_here
ALIYUN_ACCESS_KEY_SECRET=your_aliyun_access_key_secret_here
ALIYUN_OSS_ENDPOINT=oss-cn-hangzhou.aliyuncs.com
ALIYUN_OSS_BUCKET=yt-summerizer-aliyun

# 千问API配置
QWEN_API_BASE=https://dashscope-intl.aliyuncs.com/compatible-mode/v1/chat/completions

# 安全设置
SECRET_KEY=your_secret_key_here

# 其他配置
STORAGE_DIR=./storage
LOG_LEVEL=INFO
TEMP_DIR=/tmp/video_analysis

# Proxy配置
https_proxy=http://127.0.0.1:33210
http_proxy=http://127.0.0.1:33210
all_proxy=socks5://127.0.0.1:33211

# Browser Runtime
AGENTGO_API_KEY=your_agentgo_api_key_here

# Task Configuration
MAX_VIDEO_DURATION=600
TRANSCRIPTION_TIMEOUT=300
POLL_INTERVAL=5

# CORS Configuration
CORS_ORIGINS=http://localhost:5173,http://localhost:3000
